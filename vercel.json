{
  "version": 2,
  "builds": [
    {
      "src": "/json-api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dist/assets/(.*).js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ]
}
